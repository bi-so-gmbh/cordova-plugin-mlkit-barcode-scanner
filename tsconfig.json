{
  "extends": "./node_modules/mobisys-ts-config/tsconfig.json",
  "include": ["src/*.ts"],
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "esNext",
    "target": "ES5",
    "lib": ["dom", "es2017"],
    "outDir": "www",
    "declaration": true,
    "declarationDir": "./types",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "removeComments": true,
    "newLine": "lf"
  }
}
